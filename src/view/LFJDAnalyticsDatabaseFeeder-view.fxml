<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" style="-fx-background-color: #232F34;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LFJDAnalyticsDatabaseFeederController">
    <children>
        <Label maxWidth="1.7976931348623157E308" styleClass="visible-label" text="Choose a period :">
            <VBox.margin>
                <Insets bottom="5.0" left="50.0" top="15.0" />
            </VBox.margin></Label>
        <HBox>
            <children>
                <Label minWidth="40.0" styleClass="visible-label" text="From:">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </Label>
                <DatePicker fx:id="fromDatePicker" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin></DatePicker>
                <Button fx:id="btnGenerate" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#generate" styleClass="visible-button" text="Generate" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="5.0" right="10.0" />
                    </HBox.margin></Button>
            </children>
            <VBox.margin>
                <Insets bottom="5.0" />
            </VBox.margin>
        </HBox>
        <HBox>
            <children>
                <Label minWidth="40.0" styleClass="visible-label" text="To:">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin>
                </Label>
                <DatePicker fx:id="toDatePicker" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets right="10.0" />
                    </HBox.margin></DatePicker>
                <Button fx:id="btnClose" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#closeStage" styleClass="visible-button" text="Cancel" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="5.0" right="10.0" />
                    </HBox.margin></Button>
            </children>
            <VBox.margin>
                <Insets bottom="5.0" />
            </VBox.margin>
        </HBox>
        <VBox prefHeight="200.0" prefWidth="100.0">
            <children>
                <Label styleClass="visible-label" text="Result:">
                    <VBox.margin>
                        <Insets bottom="5.0" left="50.0" />
                    </VBox.margin>
                </Label>
                <TextArea fx:id="taResult" minHeight="300.0" minWidth="400.0">
                    <VBox.margin>
                        <Insets bottom="10.0" left="50.0" right="10.0" />
                    </VBox.margin>
                </TextArea>
                <ProgressBar fx:id="pgbResult" maxWidth="1.7976931348623157E308" minHeight="15.0" progress="0.0" VBox.vgrow="ALWAYS">
                    <VBox.margin>
                        <Insets bottom="10.0" left="50.0" right="10.0" />
                    </VBox.margin>
                </ProgressBar>
            </children></VBox>
    </children>
   <stylesheets>
      <URL value="@../resources/css/button.css" />
      <URL value="@../resources/css/label.css" />
   </stylesheets>
</VBox>
