<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" style="-fx-background-color: #232F34;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LFJDAnalyticsDatabaseFeederController">
    <children>
        <HBox>
            <children>
                <Label minWidth="40.0" prefWidth="60.0" styleClass="visible-label" text="Date From:">
               <HBox.margin>
                  <Insets left="10.0" top="15.0" />
               </HBox.margin></Label>
                <DatePicker fx:id="fromDatePicker" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="15.0" />
                    </HBox.margin>
            </DatePicker>
            </children>
        </HBox>
        <HBox>
            <children>
                <Label minWidth="40.0" prefWidth="60.0" styleClass="visible-label" text="Date To:">
               <HBox.margin>
                  <Insets left="10.0" />
               </HBox.margin></Label>
                <DatePicker fx:id="toDatePicker" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" />
                    </HBox.margin>
            </DatePicker>
            </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
        </HBox>
      <HBox>
         <children>
                <Button fx:id="btnGenerate" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#generate" styleClass="visible-button" text="Generate" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="80.0" right="10.0" />
                    </HBox.margin>
            </Button>
                <Button fx:id="btnClose" alignment="CENTER" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#closeStage" styleClass="visible-button" text="Cancel" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets bottom="10.0" left="5.0" right="10.0" />
                    </HBox.margin>
            </Button>
         </children>
      </HBox>
        <VBox fx:id="vBoxLog">
            <children>
                <Label styleClass="visible-label" text="Log:">
                    <VBox.margin>
                        <Insets bottom="5.0" left="10.0" />
                    </VBox.margin>
                </Label>
                <TextArea fx:id="taResult" minHeight="300.0" minWidth="400.0">
                    <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" />
                    </VBox.margin>
                </TextArea>
                <ProgressBar fx:id="pgbResult" maxWidth="1.7976931348623157E308" minHeight="15.0" progress="0.0" VBox.vgrow="ALWAYS">
                    <VBox.margin>
                        <Insets bottom="15.0" left="10.0" right="10.0" />
                    </VBox.margin>
                </ProgressBar>
            </children></VBox>
    </children>
   <stylesheets>
      <URL value="@../resources/css/button.css" />
      <URL value="@../resources/css/label.css" />
   </stylesheets>
</VBox>
